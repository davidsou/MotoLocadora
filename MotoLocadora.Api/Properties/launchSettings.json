        "ASPNETCORE_ENVIRONMENT": "Development",
        "AWS_ACCESS_ID": "",
        "AWS_ACCESS_SECRET": ""
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5006"
    },
    "Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "AWS_ACCESS_ID": "",
        "AWS_ACCESS_SECRET": ""
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7284;http://localhost:5006"
    },
    "Staging": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "AWS_ACCESS_ID": "",
        "AWS_ACCESS_SECRET": ""
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
